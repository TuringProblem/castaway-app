public class Water { 
  private int temp;
  private int tide;
  private int phLevel;

  public Water() {
    temp = 0;
    tide = 0;
    phLevel = 0;
  } 

  public Water(int temp) {
    this.temp = temp;
    tide = 0;
    phLevel = 0;
  }

  Public Water(int temp, int tide) {
    this.temp = temp;
    this.tide = tide;
    phLevel = 0;
  }

  public Water(int temp, int tide, int phLevel) {
    this.temp = temp;
    this.tide = tide;
    this.phLevel = phLevel;
  }
  //is there a storm??
}
